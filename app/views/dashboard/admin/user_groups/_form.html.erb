<div class="list">
  <%= form_for [:dashboard,:admin,user_group] do |f| %>
  <%= f.hidden_field :user_id %>
  <div class="form-inline">
    <%= f.label :group_id %>
    <%= f.select(:group_id, Group.all.collect {|u| [u.name, u.id]}, { :prompt => '請選擇'}, :class => 'form-control' ) %>
  </div>
    <%= f.submit "確認", class: " btn btn-success" %>
  <% end %>
</div>
