<% content_for :breadcrumb do %>
  <li><%= link_to "文章分類", dashboard_admin_art_kinds_path %></li>
  <li class="active">修改文章分類 | <%= @art_kind.name %></li>
<% end %>

<%= form_for [:dashboard,:admin,@art_kind] do |f| %>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.submit "確認", :disable_with => '處理中...', class: "btn btn-success" %>
<% end %>
