<%= form_for [:dashboard,:admin,group] do |f| %>
  <%= render 'shared/errors', object: f.object %>
  <div class="list padding">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control", placeholder: "名稱" %>
    <%= render 'shared/check', check: group.admin, check_name: 'admin', f: f, title: ['使用者', '管理者'] %>
    <br/>
    <%= f.submit "確認", class: " btn btn-success" %>
  </div>
<% end %>
