<ol class="breadcrumb">
  <li><%= link_to "首頁", dashboard_admin_root_path %></li>
  <li class="active">活動類型</li>
</ol>
<div class="list">
  <% @doing_kinds.each do |doing_kind| %>
    <%= doing_kind.id %>
    <%= doing_kind.title %>
    <%= link_to "修改", edit_dashboard_admin_doing_kind_path(doing_kind), class: "btn btn-sm btn-success" %>
    <%= link_to "刪除", dashboard_admin_doing_kind_path(doing_kind) , method: :delete, class: "btn btn-sm btn-danger"%><br/>
  <% end %>
</div>

<%= link_to "新增", new_dashboard_admin_doing_kind_path, class: "btn btn-success"  %>
