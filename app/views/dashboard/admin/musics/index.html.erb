<ol class="breadcrumb">
  <li><%= link_to "首頁", dashboard_admin_root_path %></li>
  <li class="active">團康音樂</li>
</ol>
<div class="col-xs-12 new">
  <%= link_to '新增', new_dashboard_admin_music_path, :class => 'btn btn-success' if reply %>
</div>

<% @musics.each do |m|%>
  <%= link_to m.title, dashboard_admin_music_path(m.id) %>
  <%= File.basename(m.file.path) %>
  <%= m.user.name %>
<% end %>
