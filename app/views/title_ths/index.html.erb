<% content_for :breadcrumb do %>
  <li class="active">各屆幹部</li>
<% end %>

<div class="list clearfix">
  <% @year.each do |y| %>
    <div class="col-xs-12"><%= y.title_year %></div>
    <div>
      <% @title_ths.each do |m| %>
        <% if y.title_year == m.title_year %>
          <div class="item">
            <%= m.member_th %>
            <%= m.title_title %>
            <%= m.member_name %>
          </div>
        <% end %>
      <% end %>
    </div>
  <% end %>
</div>
